<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                            http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.24.xsd">

    <changeSet id="001" author="suraj">
        <createTable tableName="liquibase">
            <column name="id" type="BIGINT" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="tool" type="VARCHAR(100)">
                <constraints nullable="false"/>
            </column>
            <column name="version" type="VARCHAR(20)"/>
        </createTable>
    </changeSet>

    <changeSet id="002" author="suraj">
        <sql>
            INSERT INTO liquibase (tool, version)
            VALUES ('Liquibase', '5.0.1');
        </sql>
    </changeSet>

</databaseChangeLog>

